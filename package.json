{
  "name": "tslint-rimeto",
  "version": "0.1.1",
  "description": "Rimeto tslint rules",
  "main": "tslint-rimeto.json",
  "repository": "\"git+https://github.com/rimeto/tslint-rimeto.git\"",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "clean": "rm -rf dist node_modules",
    "lint": "tslint -c ./tslint.json 'src/**/*.ts'",
    "postcheckout": "yarn",
    "postmerge": "npm run postcheckout",
    "postrebase": "npm run postcheckout",
    "prepublishOnly": "yarn build",
    "test": "yarn tslint --test src/__tests__",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@types/lodash": "^4.14.117",
    "@types/node": "^10.12.0",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "husky": "^1.1.2",
    "tslint": "^5.1.0",
    "typescript": "^2.9.0"
  }
}
